<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="./stylesheet/style.css">
    <script src="https://cdn.tailwindcss.com"></script>

</head>
<body>
    <div class="w-full h-full bg-zinc-900 p-10 text-white">
        <div class="nav mb-10">
            <a href="/" class=" text-yellow-500"> Back to Home </a>
        </div>
        <h1 class="text-3xl tracking-tighter mb-3">All Users</h1>
        <div class="users flex gap-3 flex-wrap">
            <% users.forEach(function(user){%>
                <div class="user w-72 p-4 bg-zinc-800 rounded-lg">
                    <div class="w-full h-72 bg-zinc-700 rounded-lg overflow-hidden">
                        <img src="<%= user.image %>" class="w-full h-full object-cover" alt="min">
                    </div>
                    <h3 class="text-2xl mt-3 tracking-tighter"> <%= user.name %> </h3>
                    <h5 class="text-zinc-500 tracking-tighter"><%= user.email %></h5>
                    <div class="flex gap-3">
                        <a href="" class="text-zinc-200"> Edit user</a>
                        <a href="/delete/<%= user._id %>" class="text-red-500"> Delete user</a>
                        <!-- <a href="" class="text-red-500"> Delete user</a> -->

                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</body>
</html>